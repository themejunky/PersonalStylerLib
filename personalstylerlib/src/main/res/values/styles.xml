<resources>

    <style name="cb_default_style">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/default_text_buttons</item>
    </style>

    <style name="cs_default_style" parent="Base_MediumText">
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="ci_default_style" parent="Base_MediumText">
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="Base_MediumText" parent="base_wrap_wrap">
        <item name="fontPath">Catamaran-Regular.ttf</item>
        <item name="android:textColor">@color/base_black</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="base_wrap_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="onboarding_step_4_add_service" parent="Base_MediumText">
        <item name="android:textColor">@color/base_selected</item>
        <item name="android:text">@string/onboarding_step_4_add</item>
    </style>

    <style name="base_match_match">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="default_LL_match_match_vertical">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>

    <!-- CustomViews -->
    <style name="Custom_Button_View">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">0.05</item>
    </style>

    <style name="Custom_Button_Shadow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">0.95</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:gravity">bottom</item>
        <item name="android:background">@drawable/custom_round_corrners</item>
    </style>

    <style name="Custom_Button_Text" parent="base_match_match">
        <item name="android:background">@drawable/custom_round_corrners</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:layout_marginRight">1dp</item>
        <item name="android:layout_marginBottom">3dp</item>
        <item name="android:paddingBottom">3dp</item>
    </style>

    <style name="MainSchedule_RelativeContainer" parent="base_match_wrap">
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
    </style>

    <style name="base_match_wrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="default_LL_match_wrap_vertical" parent="base_match_wrap">
        <item name="android:orientation">vertical</item>
    </style>


    <style name="MainHome_CustomItem_title" parent="Base_MediumText">
        <item name="fontPath">Catamaran-Bold.ttf</item>
        <item name="android:textColor">@color/CustomHomeItem_title</item>
    </style>

    <style name="MainHome_CustomItem_number" parent="base_wrap_wrap">
        <item name="fontPath">Catamaran-Regular.ttf</item>
        <item name="android:textColor">@color/CustomHomeItem_number</item>
        <item name="android:textSize">35sp</item>
        <item name="android:layout_marginRight">13dp</item>
        <item name="android:layout_marginTop">-12dp</item>
    </style>

    <style name="MainHome_CustomItem_text" parent="Base_MediumText">
        <item name="fontPath">Catamaran-Regular.ttf</item>
        <item name="android:textColor">@color/CustomHomeItem_title</item>
    </style>

    <style name="Register_Mandatory" parent="base_wrap_wrap">
        <item name="fontPath">Catamaran-Bold.ttf</item>
        <item name="android:textColor">#880cff</item>
        <item name="android:text">@string/register_mandatory</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="Login_Error" parent="Base_SmallText">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:visibility">gone</item>
        <item name="android:textColor">@color/default_error</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="fontPath">Catamaran-Bold.ttf</item>
    </style>

    <style name="Base_SmallText" parent="base_wrap_wrap">
        <item name="fontPath">Catamaran-Regular.ttf</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="TimePicker_Layout" parent="default_LL_wrap_wrap_vertical">
        <item name="android:layout_marginTop">60dp</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>

    <style name="app_detail_status_time" parent="base_wrap_wrap_14_regular">
    </style>

    <style name="base_wrap_wrap_14_regular" parent="base_wrap_wrap">
        <item name="android:textSize">14sp</item>
        <item name="fontPath">Catamaran-Regular.ttf</item>
        <item name="android:textColor">@color/base_black</item>
    </style>

    <style name="app_detail_status_date" parent="base_wrap_wrap_14_regular">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/base_gray</item>
    </style>

    <style name="default_LL_wrap_wrap_vertical" parent="base_wrap_wrap">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="app_detail_status_text" parent="base_wrap_wrap_14_regular">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/base_unselected_text</item>
        <item name="android:layout_marginLeft">25dp</item>
    </style>

    <style name="onboarding_step_3_service_text" parent="base_wrap_wrap">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/base_black</item>
    </style>


    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
