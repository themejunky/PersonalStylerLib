<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/nContainer"
    style="@style/MatchMatch.Horizontal">

    <ImageView
        android:id="@+id/nIcon"
        style="@style/Cube25"
        android:layout_marginRight="10dp"
        android:layout_gravity="center"/>
    
    <TextView
        android:id="@+id/nTitle"
        style="@style/WrapWrap.Regular.Black.15"
        android:paddingTop="13dp"
        android:paddingBottom="13dp"
        />

    <TextView
        android:id="@+id/nSubtext"
        style="@style/WrapWrap.Regular.Grey.14"
        android:padding="13dp"
        android:gravity="right"
        />

    <ImageView
        android:id="@+id/nArrow"
        android:layout_width="14dp"
        android:layout_height="25dp"
        android:src="@drawable/ic_menu_arrow"
        android:layout_marginRight="13dp"
        android:layout_gravity="center_vertical"
        />

</LinearLayout>